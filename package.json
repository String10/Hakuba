{
	"name": "svelte-blog",
	"version": "0.0.2",
	"scripts": {
		"dev": "vite dev",
		"generateData": "tsx .scripts/pre/index.ts",
		"build": "tsx .scripts/pre/index.ts && vite build && tsx .scripts/post/index.ts && cp -r static/* build",
		"report": "tsx .scripts/pre/index.ts && vite build && tsx .scripts/post/index.ts && cp -r static/* build",
		"package": "svelte-kit package",
		"preview": "vite preview",
		"prepare": "svelte-kit sync",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check --plugin-search-dir=. . && eslint .",
		"format": "prettier --write --plugin-search-dir=. .",
		"source": "tsx src/lib/preSource/index.ts"
	},
	"devDependencies": {
		"@fortawesome/fontawesome-free": "^6.5.1",
		"@giscus/svelte": "^2.4.0",
		"@playwright/test": "^1.41.2",
		"@sveltejs/adapter-static": "next",
		"@sveltejs/kit": "next",
		"@tailwindcss/typography": "^0.5.10",
		"@types/eslint": "^8.56.2",
		"@types/gtag.js": "^0.0.19",
		"@types/node": "^20.14.12",
		"@types/prettier": "^2.7.3",
		"@types/rollup-plugin-visualizer": "^4.2.3",
		"@typescript-eslint/eslint-plugin": "^5.62.0",
		"@typescript-eslint/parser": "^5.62.0",
		"autoprefixer": "^10.4.17",
		"clsx": "^2.1.0",
		"dotenv": "^16.4.4",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"feed": "^4.2.2",
		"mdsvex": "^0.11.0",
		"node-fetch": "^3.3.2",
		"path-to-regexp": "^6.2.1",
		"postcss": "^8.4.35",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.1",
		"prettier-plugin-tailwindcss": "^0.4.1",
		"rehype-add-classes": "^1.0.0",
		"rehype-autolink-headings": "^7.1.0",
		"rehype-external-links": "^3.0.0",
		"rehype-katex-svelte": "^1.2.0",
		"rehype-slug": "^6.0.0",
		"remark-footnotes": "2.0",
		"remark-github": "^12.0.0",
		"remark-math": "3.0.0",
		"rollup-plugin-visualizer": "^5.12.0",
		"svelte": "^3.59.2",
		"svelte-check": "^3.6.4",
		"svelte-preprocess": "^5.1.3",
		"svelte-sitemap": "^2.6.0",
		"tailwindcss": "^3.4.3",
		"tslib": "^2.6.2",
		"tsx": "^4.7.1",
		"typescript": "^5.3.3",
		"vite": "^2.9.17",
		"yaml": "^2.3.4"
	},
	"type": "module"
}
